CREATE TABLE IF NOT EXISTS `pix_date` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `field` VARCHAR(5) NOT NULL,
  `target_id` INT NOT NULL,
  `date_txt` VARCHAR(100) NOT NULL,
  `date_val` DOUBLE NOT NULL,
  `source` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`id`),
  INDEX `IX_FIELD` (`field` ASC) VISIBLE);
